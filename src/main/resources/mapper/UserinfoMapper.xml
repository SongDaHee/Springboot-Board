<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.muhayu.dhsongboard.mapper.UserinfoMapper">

    <select id="userinfoList" resultType="com.muhayu.dhsongboard.model">
        SELECT * FROM userinfo
    </select>

    <insert id="userinfoInsert" parameterType="com.muhayu.dhsongboard.model">
        insert into userinfo('id','email','password','name')
        values (#{id}, #{email}, #{password}, #{name})
    </insert>
</mapper>